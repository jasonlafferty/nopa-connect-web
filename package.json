{
  "name": "nopa-connect",
  "version": "1.0.0",
  "description": "Starter kit for React and Redux Pluralsight course by Cory House",
  "scripts": {
    "prestart": "if [ ! -d 'node_modules' ]; then npm install; fi && babel-node src/app/server/bootstrap.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node src/app/server/index.js",
    "lint": "node_modules/.bin/esw webpack.config.* app",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Issa Shabo <issa.shabo@zopa.com>",
  "dependencies": {
    "babel-polyfill": "6.8.0",
    "bootstrap": "3.3.6",
    "url-loader": "^0.5.8",
    "css-loader": "^0.27.3",
    "history": "^4.6.1",
    "jquery": "2.2.3",
    "keycode": "^2.1.8",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "4.4.5",
    "react-router": "^2.4.0",
    "redux": "3.5.2",
    "redux-thunk": "2.0.1",
    "style-loader": "^0.14.1"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "colors": "1.1.2",
    "css-loader": "^0.27.3",
    "jest": "^19.0.2",
    "enzyme": "^2.7.1",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "expect": "1.19.0",
    "express": "4.13.4",
    "json-loader": "^0.5.4",
    "node-cmd": "^2.0.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "1.8.0",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.0.2",
    "react-test-renderer": "^15.4.2",
    "redux-immutable-state-invariant": "1.2.3",
    "sass-loader": "^6.0.3",
    "webpack": "^1.13.0",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ishabo/nopa-connect"
  }
}
